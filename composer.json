{
    "name": "cesg/jwt-token-guard",
    "description": "Laravel simple JWT token guard",
    "type": "library",
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.5|^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "license": "MIT",
    "authors": [{
        "name": "Cristian SG",
        "email": "cesgbla@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Cesg\\Jwt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cesg\\Jwt\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cesg\\Jwt\\JwtTokenServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
